#!/bin/csh -f

setenv ICE_MACHINE_ENVNAME testmachine
setenv ICE_MACHINE_COMPILER intel
setenv ICE_MACHINE_MAKE gmake
setenv ICE_MACHINE_WKDIR ~/ICEPACK_RUNS
